<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rlt_term"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <CheckedTextView
        android:id="@+id/tv_reg_all_check"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:fontFamily="@font/spoqahansansneomedium"
        android:textSize="15dp"
        android:textColor="#111111"
        android:letterSpacing="-0.03"
        android:lineSpacingExtra="5sp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:drawableStart="@drawable/ask_checkbox_selector"
        android:drawablePadding="6dp"
        android:text="약관 전체 동의"
        android:background="@drawable/bg_round_default_btn_line"
        android:paddingTop="13dp"
        android:paddingStart="13dp"
        android:paddingEnd="13dp"
        android:paddingBottom="13dp"
        />

</RelativeLayout>